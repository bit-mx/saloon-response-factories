{
  "name": "bit-mx/saloon-response-factories",
  "description": "Test saloon requests easily",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BitMx\\SaloonResponseFactories\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitMx\\SaloonResponseFactories\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jorge Enr√≠quez",
      "email": "44071711+jenriquez-bit@users.noreply.github.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "saloonphp/saloon": "^3.0",
    "illuminate/support": "^10.0 || ^11.0",
    "illuminate/console": "^10.0 || ^11.0",
    "fakerphp/faker": "^1.23"
  },
  "require-dev": {
    "laravel/pint": "^1.15",
    "phpstan/phpstan": "^1.10",
    "tomasvotruba/type-coverage": "^0.2.5",
    "phpstan/extension-installer": "^1.3",
    "pestphp/pest": "^2.34",
    "orchestra/testbench": "^9.0",
    "pestphp/pest-plugin-laravel": "^2.3"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BitMx\\SaloonResponseFactories\\SaloonResponseFactoriesServiceProvider"
      ]
    }
  }
}
